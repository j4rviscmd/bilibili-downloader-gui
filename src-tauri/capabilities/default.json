{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "core:window:allow-set-theme",
    "process:default",
    "opener:default",
    "shell:allow-open",
    "dialog:default",
    {
      "identifier": "opener:allow-open-url",
      "allow": [
        {
          "url": "https://www.bilibili.com"
        }
      ]
    }
  ]
}
